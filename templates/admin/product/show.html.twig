{% extends "admin/base_admin.html.twig" %}

{% block body %}
    <h1>Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ product.name }}</td>
            </tr>
            <tr>
                <th>ImagePath</th>
                <td><img src="{{ asset(product.imagePath) }}" alt=""></td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('product_index') }}">back to list</a>


{% endblock %}
